<%= form_for @solicitation do |f| %>
  <div class="input-field">
    <%= f.label :subject, 'Assunto' %>
    <%= f.text_field :subject %>
  </div>
  <div class="input-field">
    <%= f.label :content, 'Descreva a solicitação' %>
    <%= f.text_area :content, class:'materialize-textarea' %>
  </div>
  <div class="input-field center-align">
    <%= f.submit 'Enviar Solicitação', class: 'btn' %>
  </div>
<% end %>
